$(function () {
    $('##{champTableId}').tablesorter({ sortList: [[0,0]] });

    $('#customize-toggle').click(function () {
        var $button = $(this);
        $('#summoner-form').removeClass('default-form').toggle('fast', function () {
            if ($(this).is(':visible')) {
                $button.find('.plusminus').html('-');
            } else {
                $button.find('.plusminus').html('+');
            }
        });
    });
});
